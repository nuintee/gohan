{
  "name": "gohan",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:exposed": "NODE_OPTIONS='--inspect' next dev -H 192.168.31.207",
    "build": "next build",
    "start": "next start",
    "lint": "next lint  --dir src",
    "lint:fix": "next lint --dir src --fix",
    "prettier:fix": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest --watch",
    "test:ci": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "server:mock": "json-server --watch src/json-server/db.json",
    "prepare": "husky install",
    "commit": "npx git-cz",
    "addfc": "npx hygen fc new",
    "prisma:push": "npx prisma db push",
    "prisma:gen": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:all": "npm run prisma:push && npm run prisma:gen && npm run prisma:studio",
    "deadcode": "ts-prune -e",
    "deadcode:count": "ts-prune | wc -l"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix"
    ]
  },
